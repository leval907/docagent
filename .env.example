# DocAgent Pipeline Configuration
# Скопируйте в .env и заполните значения

# === PostgreSQL Configuration ===
PG_HOST=localhost
PG_PORT=5432
PG_DATABASE=docagent
PG_USER=postgres
PG_PASSWORD=your_password_here

# === S3 Configuration ===
# Для AWS S3
# S3 Storage Configuration (Beget Cloud)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
S3_ENDPOINT=https://s3.ru1.storage.beget.cloud
S3_BUCKET=your-bucket-name

# Пример для Beget S3:
# AWS_ACCESS_KEY_ID=JQDHVXZY7XFWUHF8LV0S
# AWS_SECRET_ACCESS_KEY=pjVG1Zt5G6y8N8eYAmPnKcnnPpfxB3KVCcFrEyfk
# S3_BUCKET=db6a1f644d97-la-ducem1

# Для MinIO или другого S3-совместимого хранилища
# S3_ENDPOINT=http://localhost:9000

# === Optional: Python encoding (для Windows) ===
PYTHONIOENCODING=utf-8
